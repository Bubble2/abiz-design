{
  "name": "abiz-design",
  "version": "0.0.2",
  "scripts": {
    "clean": "lerna clean --yes",
    "bootstrap": "lerna bootstrap",
    "init": "npm install && npm run clean && npm run bootstrap",
    "publish": "lerna publish",
    "publish:git": "lerna publish from-git",
    "publish:package": "lerna publish from-package",
    "docs:dev": "dumi dev",
    "docs:build": "dumi build --mode=production",
    "build": "lerna run core-build && father-build && lerna run aeps-build && lerna run jxc-build && lerna run miccn-build",
    "build:icons-aeps": "lerna run icons-aeps-build",
    "build:icons-jxc": "lerna run icons-jxc-build",
    "build:icons-miccn": "lerna run icons-miccn-build",
    "build:core": "lerna run core-build",
    "build:aeps": "lerna run aeps-build",
    "build:jxc": "lerna run jxc-build",
    "build:miccn": "lerna run miccn-build",
    "release": "npm run build && npm run publish",
    "prettier": "prettier --write \"**/*.md\"",
    "test": "umi-test",
    "test:update": "umi-test --updateSnapshot",
    "test:coverage": "umi-test --coverage",
    "fix-memory-limit": "cross-env LIMIT=4096 increase-memory-limit",
    "link": "lerna link",
    "generatorDocs": "node generatorDemo.js && npm run prettier && node generatorDocs.js"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@umijs/fabric": "^2.4.11",
    "antd": "^4.9.4",
    "array-move": "^3.0.1",
    "dumi-assets-types": "^1.0.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "fetch-jsonp": "^1.1.3",
    "immutability-helper": "^3.1.1",
    "rc-tween-one": "^2.7.3",
    "react": "^16.13.1",
    "react-dnd": "^11.1.3",
    "react-dnd-html5-backend": "^11.1.3",
    "react-dom": "^16.13.1",
    "react-draggable": "^4.4.3",
    "react-highlight-words": "^0.17.0",
    "react-infinite-scroller": "^1.2.4",
    "react-resizable": "^1.11.0",
    "react-router-dom": "^5.0.1",
    "react-sortable-hoc": "^1.11.0",
    "react-sticky": "^6.0.3",
    "react-text-loop": "^2.3.0",
    "react-virtualized": "^9.22.3",
    "react-window": "^1.8.6",
    "reqwest": "^2.0.5"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.12.17",
    "@umijs/plugin-esbuild": "^1.1.0",
    "@umijs/test": "^3.0.5",
    "babel-plugin-import": "^1.13.3",
    "cross-env": "^7.0.3",
    "dumi": "^1.0.13",
    "father-build": "^1.19.1",
    "gh-pages": "^3.0.0",
    "increase-memory-limit": "^1.0.7",
    "lerna": "^3.22.1",
    "less-vars-to-js": "^1.3.0",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "walk-sync": "^2.2.0",
    "yorkie": "^2.0.0"
  }
}
